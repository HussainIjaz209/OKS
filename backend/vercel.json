{
    "version": 2,
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.js"
        },
        {
            "src": "/uploads/(.*)",
            "dest": "/uploads/$1"
        }
    ],
    "functions": {
        "api/index.js": {
            "maxDuration": 30
        }
    }
}